#include <xc.h>

void main(void) {

    int arr1[5] = {2,1,4,3,5};
    int arr2[5];
    
    
    int tmp;
    
    for(int i=0;i<5;i++) {
    
        for(int j=0;j<5;j++) {
        
            if(arr1[j] > arr1[j]) {
            
                tmp = arr1[j];
                arr1[j] = arr1[j+1];
                arr1[j+1] = tmp;
            }
        }
    }
    
    
    for(int i=0;i<5;i++) {
        arr2[i] = arr1[i]; 
    }
    
     return;
   
}
